                                 ;==============================================================================
                                 ; Delay of 1 s (including RCALL)
                                 ;==============================================================================
                                 delay_1_micros:
0000de 0000                      	NOP
0000df 0000                      	NOP
0000e0 0000                      	NOP
0000e1 0000                      	NOP
0000e2 0000                      	NOP
0000e3 0000                      	NOP
0000e4 0000                      	NOP
0000e5 0000                      	NOP
0000e6 0000                      	NOP
0000e7 0000                      	NOP
0000e8 0000                      	NOP
0000e9 0000                      	NOP
                                 
0000ea 9508                      	RET
                                 
                                 ;==============================================================================
                                 ; Delay of X s
                                 ;	LDI + RCALL = 4 cycles
                                 ; Uses registers:
                                 ;	R24				Input parameter data (X s)
                                 ;==============================================================================
                                 delay_micros:
                                 	
0000eb dff2                      	RCALL delay_1_micros
0000ec 958a                      	DEC R24
0000ed f7e9                      	BRNE delay_micros
0000ee 9508                      	RET
                                 
                                 ;==============================================================================
                                 ; Delay of X ms
                                 ;	LDI + RCALL = 4 cycles
                                 ; Uses registers:
                                 ;	R18				Copy of parameter data (X ms)
                                 ;	R24				Input parameter data (X ms) and
                                 ;					also input to 'delay_micros'.
                                 ;==============================================================================
                                 delay_ms:
                                 
                                 
0000ef 2f28                      	MOV			R18,		R24			; Copy the value in R24 to R18
                                 
                                 L1:
0000f0 ef8a                      	LDI			R24,		0xFA		; R24=250	
                                 
0000f1 dff9                      	RCALL		delay_micros			; 250 micros delayed
                                 
0000f2 ef8a                      	LDI			R24,		0xFA		; R24=250	
                                 
0000f3 dff7                      	RCALL		delay_micros			; 500 micros delayed
                                 
0000f4 ef8a                      	LDI			R24,		0xFA		; R24=250	
                                 
0000f5 dff5                      	RCALL		delay_micros			; 750 micros delayed
                                 
0000f6 ef8a                      	LDI			R24,		0xFA		; R24=250	
                                 
0000f7 dff3                      	RCALL		delay_micros			; 1000 micros delayed = 1ms
                                 
0000f8 952a                      	DEC		R18
                                 
0000f9 f7b1                      	BRNE	L1
                                 
0000fa 9508                      	RET
                                 
                                 ;==============================================================================
                                 ; Delay of 1 s
                                 ;	LDI + RCALL = 4 cycles
                                 ; Uses registers:
                                 ;	R24				Used for storage and
                                 ;					also input to 'delay_micros'.
                                 ;==============================================================================
                                 delay_1_s:
                                 
0000fb ef8a                      	LDI			R24,		0xFA		; R24=250	
                                 
0000fc dff2                      	RCALL		delay_ms				; 250 ms delayed
                                 
0000fd ef8a                      	LDI			R24,		0xFA		; R24=250	
                                 
0000fe dff0                      	RCALL		delay_ms				; 500 ms delayed
                                 
0000ff ef8a                      	LDI			R24,		0xFA		; R24=250	
                                 
000100 dfee                      	RCALL		delay_ms				; 750 ms delayed
                                 
000101 ef8a                      	LDI			R24,		0xFA		; R24=250	
                                 
000102 dfec                      	RCALL		delay_ms				; 1000 ms delayed = 1s
                                 
000103 9508                      	RET
                                 
                                 ;==============================================================================
                                 ; Delay of X s
                                 ;	LDI + RCALL = 4 cycles
                                 ; Uses registers:
                                 ;	R18				Input parameter data (X ms)
                                 ;==============================================================================
                                 
                                 delay_s:
000104 dff6                      	RCALL		delay_1_s				; delay_1_s
000105 952a                      	DEC			R18						; decrement counter
000106 f7e9                      	BRNE		delay_s					; start over if not 0
000107 9508                      	RET
