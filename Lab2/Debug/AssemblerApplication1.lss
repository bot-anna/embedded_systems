                                 	.MACRO			CLR_IO_BIT
                                 	LDS				R24,			@0				; Read reg, store in R24
                                 	ANDI			R24,			@1				; Mask bits, store in R24
                                 	STS				@0,				R24				; Set register
                                 	.ENDMACRO
                                 
