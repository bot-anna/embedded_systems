                                 ;==============================================================================
                                 	.MACRO			SET_IO_BIT
                                 	LDS				R24,			@0				; Read reg, store in R24
                                 	ORI				R24,			@1				; Mask bits, store in R24
                                 	STS				@0,				R24				; Set register
                                 	.ENDMACRO
                                 
                                 ;==============================================================================
                                 ; Macro for clearing a bit in I/O register, located in the extended I/O space.
